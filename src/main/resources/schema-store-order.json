{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "store/order",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "description": "Order Id"
        },
        "petId": {
            "type": "integer",
            "description": "Pet Id"
        },
        "quantity": {
            "type": "integer",
            "description": "Quantity"
        },
        "shipDate": {
            "type": "string",
            "description": "Ship Date"
        },
        "status": {
            "type": "string",
            "description": "Status"
        },
        "complete": {
            "type": "boolean",
            "description": "Complete?"
        }
    }
}